import 'package:flutter/material.dart';
import 'pages/home_page.dart';

void main() {
  runApp(const MaTahminApp());
}

class MaTahminApp extends StatelessWidget {
  const MaTahminApp({super.key});

  @override
  Widget build(BuildContext context) {
    return MaterialApp(
      title: 'Ma-Tahmin',
      debugShowCheckedModeBanner: false,
      theme: ThemeData(
        primarySwatch: Colors.blue,
        useMaterial3: true,
      ),
      home: const HomePage(),
    );
  }
}
